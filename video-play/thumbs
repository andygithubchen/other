 preload="auto">
  <source src="https://www.nuevolab.com/vdjs/media/surf_240.mp4" res="240" label="240p" type="video/mp4">
  <source src="https://www.nuevolab.com/vdjs/media/surf_360.mp4" res="360" label="360p" type="video/mp4">
